# .env file

# Ports
POSTGRES_RESOURCE_PORT=5433:5432
POSTGRES_SONG_PORT=5434:5432
POSTGRES_STORAGE_PORT=5435:5432
EUREKA_PORT=8761
RABBITMQ_PORT=5672
LOCALSTACK_PORT=4566
RESOURCE_PORT=8081
SONG_PORT=8082
PROCESSOR_PORT=8082
API_GATEWAY_PORT=8080
